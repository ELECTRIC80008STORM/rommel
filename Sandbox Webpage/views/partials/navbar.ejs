<nav>
    <div class="nav-wrapper">
        <% for(privilege of privileges){ %>
            <% if(privilege.permission == 'viewAdminFunctions'){ %>
                <a href="/user/admin" class="brand-logo">Admin</a>
            <% } %>
        <% } %>

        <ul id="nav-mobile" class="right hide-on-med-and-down">
            <% if (typeof isUserRegistered !== 'undefined' && isUserRegistered && typeof userProfileView !== 'undefined' && typeof username !== 'undefined') { %>
                <li><a href="<%= userProfileView %>"><%= username %></a></li>
            <% } else { %>
                <li><a href="/user/signin">Sign In</a></li>
                <li><a href="/user/signup">Sign Up</a></li>
            <% } %>

            <% for(privilege of privileges){ %>
                <% if(privilege.permission == 'viewAdminFunctions'){ %>
                    <li><a href="/uploaded-image">Upload Your Own Favorite Animal Image</a></li>
                <% } %>
            <% } %>

            <% for(privilege of privileges){ %>
                <% if(privilege.permission == 'viewCatsPages'){ %>
                    <li><a href="/cat">Cats</a></li>
                <% } %>
            <% } %>

            <% for(privilege of privileges){ %>
                <% if(privilege.permission == 'viewDogsPages'){ %>
                    <li><a href="/dog">Dogs</a></li>
                <% } %>
            <% } %>

        </ul>
    </div>
</nav>